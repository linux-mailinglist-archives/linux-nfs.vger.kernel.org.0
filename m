>  config RPCSEC_GSS_KRB5
>  	tristate "Secure RPC: Kerberos V mechanism"
> -	depends on SUNRPC && CRYPTO
> +	depends on SUNRPC
>  	default y
>  	select SUNRPC_GSS
> +	select CRYPTO
>  	select CRYPTO_SKCIPHER
>  	select CRYPTO_HASH
>  	help
>  	  Choose Y here to enable Secure RPC using the Kerberos version 5
>  	  GSS-API mechanism (RFC 1964).
